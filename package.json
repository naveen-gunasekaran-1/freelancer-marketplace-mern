{
  "name": "freelance-marketplace",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && node index.js",
    "client": "vite",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "cd server && npm install --production",
    "start": "NODE_ENV=production node server/index.js",
    "start:prod": "NODE_ENV=production PORT=3001 node server/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:regression": "node tests/regression/runAll.js",
    "test:regression:auth": "node tests/regression/auth.test.js",
    "test:regression:auth:improved": "node tests/regression/auth.improved.test.js",
    "test:regression:diagnose": "node tests/regression/diagnose.js",
    "test:regression:jobs": "node tests/regression/jobs.test.js",
    "test:regression:messaging": "node tests/regression/messaging.test.js",
    "test:regression:headless": "HEADLESS=true node tests/regression/runAll.js",
    "test:jobs:create": "node tests/regression/createTestJobs.js",
    "test:jobs:cleanup": "node tests/regression/cleanupTestJobs.js",
    "docker:build": "docker build -t freelancer-marketplace .",
    "docker:run": "docker run -p 3001:3001 freelancer-marketplace",
    "docker:compose": "docker-compose up -d",
    "docker:compose:build": "docker-compose up --build -d",
    "docker:compose:down": "docker-compose down",
    "postinstall": "npm run build:client"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "@zegocloud/zego-uikit-prebuilt": "^2.17.0",
    "agora-rtc-sdk-ng": "^4.24.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.3.0",
    "file-type": "^21.0.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "razorpay": "^2.9.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "redis": "^5.9.0",
    "sharp": "^0.34.4",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^14.25.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.13",
    "@types/nodemailer": "^6.4.21",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/selenium-webdriver": "^4.35.4",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "chromedriver": "^142.0.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.4.35",
    "selenium-webdriver": "^4.38.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.1",
    "terser": "^5.19.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.12"
  }
}
